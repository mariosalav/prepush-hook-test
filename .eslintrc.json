{
	"env": {
			"browser": true,
			"es2021": true
	},
	"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/recommended"
	],
	"overrides": [
		{
			"files":[ "src/**/*.ts"],
			"rules": {
				"dot-notation": "error"
			}
		}
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
	},
	"plugins": [
			"@typescript-eslint"
	],
	"rules": {
	}
}
